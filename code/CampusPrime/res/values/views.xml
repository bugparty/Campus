<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextField">
        <item name="android:textSize">@dimen/form_text_size</item>
        <item name="android:textColor">@color/sel_text</item>
        <item name="android:textColorHint">@color/sel_text_hint</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:background">@drawable/sel_edit_text_bg</item>
        <item name="android:paddingBottom">12.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8.0dip</item>
        <item name="android:inputType">text</item>
    </style>
    
    <style name="TextFieldLink" parent="@style/TextField">
        <item name="android:background">@drawable/sel_text_holo_bg</item>
        <item name="android:padding">12.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="label_text1">
        <item name="android:textSize">@dimen/form_text_size</item>
        <item name="android:textColor">@color/sel_text</item>
        <item name="android:textColorHint">@color/sel_text_hint</item>
